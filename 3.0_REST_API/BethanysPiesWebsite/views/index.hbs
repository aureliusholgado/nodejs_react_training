{{!-- this {{title}} property is from index.js --}}
<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<ul>
    {{#data}} {{!-- the # sign puts the data in a for loop --}}
    <li>{{name}}</li> {{!-- this is the data.name --}}
    {{/data}}
</ul>

<div id="table">
    <table>
        
        <thead>
            <th>ID</th>
            <th>Name</th>
            <th>Whole Price</th>
            <th>Slice Price</th>
            <th>Slice Calories</th>
            <th>Image URL</th>
        </thead>

        {{#data}}
        <tbody>
            <tr>
                <td>{{id}}</td>
                <td>{{name}}</td>
                <td>{{wholePrice}}</td>
                <td>{{slicePrice}}</td>
                <td>{{sliceCalories}}</td>
                <td>{{imageUrl}}</td>
            </tr>
        </tbody>
        {{/data}}

    </table>
</div>